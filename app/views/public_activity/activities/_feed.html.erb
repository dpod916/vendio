<% unless activity.parameters[:changes].nil? && keys[1] == 'update' %>
<div class="feed-element">
    <a href="#" class="pull-left">
        <img alt="image" class="img-circle" src="<%= avatar_url(activity.owner.profile) %>">
    </a>
    <div class="media-body ">
        <small class="pull-right"><%= time_ago_in_words(activity.created_at)  %> ago</small>
        <strong><%= link_to activity.owner.name, activity.owner %></strong>
        <%= render partial: "public_activity/#{keys[0]}/#{keys[1]}", locals: {activity: activity} %>
        <small class="text-muted"><%= activity.created_at.to_date.to_formatted_s :long_ordinal %></small>
    </div>
</div>

<% end %>