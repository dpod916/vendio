<%= simple_form_for(@vendor) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :legal_name %>
    <%= f.input :type %>
    <%= f.input :parent_id %>
    <%= f.input :lng %>
    <%= f.input :lat %>
    <%= f.input : %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
