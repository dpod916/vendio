<% @owner = @work_order %>
<% authenticated = authenticated(@work_order) %>
<div class="col-lg-9">
<div class="wrapper wrapper-content animated fadeInUp">
<div class="ibox">
<div class="ibox-content">
<div class="row">
    <div class="col-lg-12">
        <div class="m-b-md">
            <h2><%= "Work Order# #{@work_order.title} for " %><%= link_to @work_order.agreement.title, @work_order.agreement %></h2>
        </div>
    </div>
</div>
<br><br>
<div class="row">
    <div class="col-md-6" id="first_column">
        <dl class="dl-horizontal">
        <dt>Vendor:</dt>
        <dd>
            <%= link_to @work_order.vendor.name, @work_order.vendor unless @work_order.vendor.nil? %>
        </dd>
        <dt>Bunge Entity: </dt>
        <dd>
            <%= link_to @work_order.company.name, @work_order.company unless @work_order.company.nil? %>
        </dd>
               
        </dl>
    </div>
    <div class="col-md-6" id="cluster_info">
        <dl class="dl-horizontal">
        <dt>Effective Date: </dt>
        <dd>
            <%= @work_order.start_date %>
        </dd>
        <dt>Expiration: </dt>
        <dd>
            <%= @work_order.due_date %>
        </dd>
               
        </dl>
    </div>
</div>



<div class="row">
